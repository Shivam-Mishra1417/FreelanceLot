# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index(keywords:String ?="")
GET       /:key              controllers.HomeController.getSearchResults(key: String)
GET       /user/:key              controllers.HomeController.getEmployer(key: String)
GET       /skill/:skill_key               controllers.HomeController.getSkillsResult(skill_key: String)
GET       /individualStat/:p_id,:p_title,:p_descr     controllers.HomeController.getIndividualStat(p_id: String,p_title: String,p_descr: String)
GET       /globalstat/                      controllers.HomeController.getGlobalStat()
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
